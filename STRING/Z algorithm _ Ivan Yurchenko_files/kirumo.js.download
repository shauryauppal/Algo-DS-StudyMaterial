( function( $ ) {
	$( '#nav-toggle .toggle' ).click( function() {
		$( 'body' ).toggleClass( 'active-nav' );
	} );
	$('#mobile-menu .li-header').each( function() {
		$( this ).click( function() {
			$( '.arrow',  this ).toggleClass( 'open' );
			$( this ).next( 'ul' ).slideToggle( '300' );
		});
	});
} )( jQuery );